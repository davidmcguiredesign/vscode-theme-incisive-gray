      "settings": {
        "foreground": "#c63997",
        "fontStyle": ""
      }
    },
    {
      "name": "Uno4",
      "scope": "punctuation.definition.block, punctuation.definition.string, punctuation.definition.variable, punctuation.definition.string, punctuation.definition.parameters, punctuation.definition.string, punctuation.definition.array, punctuation.terminator, punctuation.separator, punctuation.section, meta.brace, meta.delimiter, string.other.link, markup.quote, meta.separator",
      "settings": {
        "foreground": "#bd75a5",
        "fontStyle": ""
      }
    },
    {
      "name": "Uno5",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#bc8fad",
        "fontStyle": ""
      }
    },
    {
      "name": "Uno1Bold",
      "scope": "keyword.control, storage",
      "settings": {
        "foreground": "#950465",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Uno2Bold",
      "scope": "markup.bold, punctuation.definition.bold, meta.selector entity.other.attribute-name.id, meta.selector entity.other.attribute-name.class, meta.selector entity.other.attribute-name.pseudo-class, meta.selector entity.name.tag",
      "settings": {
        "foreground": "#b3197f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Due1",
      "scope": "constant.other.symbol, constant.numeric, constant.language.boolean, constant, support.constant, variable.language, entity.name.tag, punctuation.definition.tag, entity.other.attribute-name, entity.other.attribute-name.id, punctuation.definition.entity, meta.link",
      "settings": {
        "foreground": "#954d04",
        "fontStyle": ""
      }
    },
    {
      "name": "Due2",
      "scope": "keyword.other.special-method, meta.definition",
      "settings": {
        "foreground": "#b36619",
        "fontStyle": ""
