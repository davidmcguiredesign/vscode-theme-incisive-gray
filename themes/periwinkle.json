    {
      "name": "Default",
      "scope": "meta.embedded",
      "settings": {
        "foreground": "#0d0d73",
        "fontStyle": ""
      }
    },
    {
      "name": "Broken",
      "scope": "invalid.broken, invalid.deprecated, invalid.unimplemented",
      "settings": {
        "foreground": "#cc0000",
        "fontStyle": "bold"
      }
    }
