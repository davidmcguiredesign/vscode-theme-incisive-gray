      "settings": {
        "foreground": "#52e08b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Uno2Italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#52e08b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Due1",
